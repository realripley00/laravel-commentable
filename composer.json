{
    "name": "realripley00/laravel-commentable",
    "description": "Add comments to any model within Larvel projects.",
    "license": "MIT",
    "homepage": "https://greenstonemedia.com",
    "keywords": ["laravel", "comments", "eloquent", "laravel5"],
    "authors": [{
        "name": "Michael burgin",
        "email": "realripley00@gmail.com"
    }],
    "require": {
        "php": ">=5.5.0",
        "illuminate/database": ">= 5.0",
        "illuminate/support": ">= 5.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "vlucas/phpdotenv": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "RealRipley\\Commentable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RealRipley\\Commentable\\CommentableServiceProvider"
            ],
            "aliases": {
                "Commentable": "RealRipley\\Commentable\\Commentable"
            }
        }
    }
}